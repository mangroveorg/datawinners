          <item>
            <label>Asparagus</label>
            <value>asparagus</value>
          </item>
          <item>
            <label>Beetroot</label>
            <value>beetroot</value>
          </item>
          <item>
            <label>Bell pepper</label>
            <value>bell_pepper</value>
          </item>
          <item>
            <label>Brocolli</label>
            <value>brocolli</value>
          </item>
          <item>
            <label>Brussels sprout</label>
            <value>brussels_sprout</value>
          </item>
          <item>
            <label>Cabbage</label>
            <value>cabbage</value>
          </item>
          <item>
            <label>Carrot</label>
            <value>carrot</value>
          </item>
          <item>
